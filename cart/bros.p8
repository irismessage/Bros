function nameaddr(i)
	-- 0x5e00 + 9 * 12
	return 0x5e6c + i * 12
end

function loadname(i)
	addr = nameaddr(i)
	name = chr(peek4(addr,3))
	return name
end

function savename(i,name)
	addr = nameaddr(i)
	n1,n2,n3 = ord(name,1,3)
	poke4(addr,n1,n2,n3)
end

function rankscore(num)
	for i=1,10 do
		if num > dget(i) then
			return i
		end
	end
	return 11
end

function shiftscores(rank)
	for i=10,rank,-1 do
		j = i-1
		score = dget(j)
		dset(i,score)
		name = loadname(j)
		savename(i,name)
	end
end

