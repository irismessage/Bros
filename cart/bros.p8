function coinup()
	g.score += 10
	g.coins += 1
	if g.coins > 99 then
		g.lives += 1
		g.coins = 0
	end
	drawtopbar()
end

function bonk()
	-- called in updatejump()
	sfx(56,1)
	if (p.x % 8 != 0) return
	mx = p.x/8
	my = p.y/8-1
	sprn = mget(mx,my)
	if sprn != bg then
		if sprn==17 then
			sfx(56,1)
			coin.x = p.x
			coin.y = p.y-16
			coin.show = true
			coin.lifet = 5*tick
			mset(mx,my,16)
		elseif sprn == 42 then
			fungus.x = p.x
			fungus.y = p.y-16
			fungus.show = true
			g.score += 100
			drawtopbar()
			mset(mx,my,16)
		elseif g.fungus and sprn==18 then
			g.score += 25
			mset(mx,my,bg)
		elseif sprn==0 then
			sfx(63,1)
		end
	end
end

function updatecoin()
	if coin.show then
		if coin.lifet == 0 then
			coin.show = false
			coinup()
		else
			coin.lifet -= 1
		end
	end
end

function updatefungus()
	if fungus.show then
		if fungus.x==p.x and fungus.y==p.y then
			g.fungus = true
			fungus.show = false
			drawtopbar()
			sfx(56)
		end
	end
end

function fguydcol()
	a,b = ycol(fguy.x,fguy.y+8)
	return ft(a) or ft(b)
end

function updatefguy()
	if (not fguy.show) return

	-- player interaction
	if abs(p.x-fguy.x) <= 8 then
		sep = p.y - fguy.y
		printh(sep)
		if sep==0 or sep==8 then
			die()
		elseif sep == -8 then
			if p.jump != 1 then
				die()
			else
				fguy.show = false
				g.score += 100
				drawtopbar()
				sfx(56)
			end
		end
	end

	-- fall
	if fguy.y > 108 then
		fguy.show = false
	end
	if not fguydcol() then
		if fguy.jtick == 0 then
			fguy.jtick = tick
			fguy.y += 8
		else
			fguy.jtick -= 1
		end
		return
	end

	-- walk
	if fguy.wtick == 0 then
		fguy.wtick = 8*tick
		if fguy.l then
			if not ft(xcol(fguy.x-8,fguy.y)) then
				fguy.x -= 4
			else
				fguy.l = false
				fguy.x += 4
			end
		else
			if not ft(xcol(fguy.x+8,fguy.y)) then
				fguy.x += 4
			else
				fguy.l = true
				fguy.x -= 4
			end
		end
	else
		fguy.wtick -=1
	end
end

function drawentities()
	if coin.show then
		spr(32,coin.x,coin.y)
	end
	if fungus.show then
		spr(41,fungus.x,fungus.y)
	end
	if fguy.show then
		spr(8,fguy.x,fguy.y,1,1,fguy.l)
	end
end

function updateentities()
	updatecoin()
	updatefungus()
	updatefguy()
end

