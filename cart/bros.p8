function levelscreenplay()
	s.play = true
	drawtopbar()
end

function decodescreen(scrn)
	scrchrs = screens[scrn]
	scrd = {ord(scrchrs,1,#scrchrs)}
	x = 0
	y = offset
	i = 1
	while y < #scrd do
		sprn = scrd[i]
		if sprn == bg then
		 i += 1
			reps = scrd[i]
			for j=1,reps do
				mset(x,y,bg)
				nextm()
			end
		else
			sprn = submtile(sprn,x,y)
			mset(x,y,sprn)
			nextm()
		end
		i += 1
	end
end

function nextm()
	x += 1
	if x > 15 then
		x = 0
		y += 1
	end
end

function submtile(sprn,x,y)
	if sprn == 1 then
		sprn = bg
		l.px = 8*x
		l.py = 8*y
	elseif sprn == 8 then
		sprn = bg
		fguy.x = 8*x
		fguy.y = 8*y
		fguy.l = true
		fguy.show = true
	end
	return sprn
end

function levelup()
	l.screen += 1
	if l.screen > 5 then
		l.screen = 1
		l.stage += 1
		g.timer = 999
	end
	if l.stage > 4 then
		l.stage = 1
		l.world += 1
	end
	drawtopbar()
	loadlevel()
end

function loadlevel()
	coin.show = false
	fungus.show = false
	fguy.show = false
	scrn = 0
	scrn += (l.world-1) * 20
	scrn += (l.stage-1) * 4
	scrn += l.screen
	if l.screen == 1 then
		music(((l.stage-1)%3)*8)
	end
	decodescreen(scrn)
	p.x = l.px
end

function checkforlevelup()
	if btn(‚û°Ô∏è) and p.wtick==0 then
		if p.x > 114 then
			levelup()
		end
	end
end

function resetp()
	p.dead = false
	p.x = l.px
	p.y = l.py
	p.l = false
	if g.timer == 0 then
		g.timer = 999
	end
end

function die()
	p.dead = true
	sfx(56)
	g.lives -= 1
	g.fungus = false
	drawtopbar()
	wait.f = 30
	if g.lives == 0 then
		wait.call = gameover
	else
