pachinko = {2,2,3,3,0,1,0,1,5,4}
codestep = 1
function updatecode()
	if (btn()==0) return
	
	req = pachinko[codestep]
	for btnid=0,5 do
		if btnp(btnid) then
			if btnid==req then
				codestep +=1
			else
				codestep = 1
				return
			end
		end
	end

	if codestep > #pachinko then
		codestep = 1
		along()
	end
end

