-- sampled sound playing
function unpacksnds()
	for k,v in pairs(snd) do
		bytes = {ord(v,1,#v)}
		samples = {}
		for b in all(bytes) do
			for shift=6,0,-2 do
				bits = (b>>shift) & 3
				add(samples,amp[bits])
			end
		end
		snd[k] = samples
	end
end

function psnd(snd)
	adr = usrdta
	for sam in all(snd) do
		poke(adr,sam)
		adr += 1
		if adr == dtaend then
			serial(0x808,usrdta,dtalen)
			while stat(108)!=0 do end
			adr = usrdta
		end
	end
	remains = adr-usrdta
	serial(0x808,usrdta,remains)
	while stat(108)!=0 do end
end

