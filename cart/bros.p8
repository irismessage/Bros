-- movement

tick = 2
-- walk
wtickl = 1 * tick
-- stop walk
-- todo
stickl = 4 * tick
-- up jump
utickl = 2 * tick
-- down fall
dtickl = 1 * tick
-- refill jump on floor
rtickl = 2 * tick
-- bonk
btickl = 4 * tick
-- apex of jump
atickl = 0 * tick

jumpmax = 5
coyotemax = 2

p = {
	x=0,
	y=0,
	l=false,
	wtick=0,
	jtick=0,
	jump=jumpmax,
	coyote=0,
}


function drawbro()
	sprn = 2
	if btn(â¬†ï¸) or btn(ğŸ…¾ï¸) then
		sprn = 5
	elseif p.wtick==1 then
		sprn = 3
	end
	y = p.y
	spr(sprn,p.x,y,1,1,p.l)
end

function updatewalk()
	if p.wtick > 0 then
		p.wtick -= 1
		return
	end

	if btn(â¬…ï¸) and btn(â¡ï¸) then
		return
	end

	if btn(â¬…ï¸) then
		p.l = true
		if lcol() or p.x<4 then
			p.wtick = stickl
		else
			p.wtick = wtickl
			p.x -= 4
		end
	end
	if btn(â¡ï¸) then
		p.l = false
		if rcol() or 124<p.x then
		 p.wtick = stickl
		else
			p.wtick = wtickl
			p.x += 4
		end
	end
end

function updatejump()
	if p.jtick > 0 then
		p.jtick -= 1
		return
	end
	p.y += 8

	p.jtick = dtickl
	if dcol() then
		p.y -= 8
		p.coyote = coyotemax
		if p.jump == 1 then
			p.jump = jumpmax
			p.jtick = rtickl
			return
		else
			p.jtick = 0
		end
	end

	if 1 < p.jump then
		if btn(â¬†ï¸) or btn(ğŸ…¾ï¸) then
			p.y -= 16
			p.jtick = utickl
			if p.jump == jumpmax then
				p.y += 8
				sfx(56)
			elseif p.jump == 3 then
				p.jtick = atickl
				p.wtick = 0
			end
			p.jump -= 1
			if ucol() then
				p.y += 8
				p.jump = 1
				p.jtick = btickl
				bonk()
			end
		elseif p.jump < jumpmax then
			p.jump = 1
			sfx(56,-2)
		end
		return
	end

	-- coyote time
	if p.jump==jumpmax and not dcol() then
		if 0 < p.coyote then
			p.coyote -= 1
		else
			p.jump = 1
		end
	end

	if p.y > 108 then
		die()
	end
end

-- collision
function ccollide(x,y)
	return mget(x/8,y/8)
end

function ft(sprn)
	return fget(sprn,0)
end

function ycol(x,y)
	if x % 8 == 0 then
		return ccollide(x,y),bg
	else
		a = ccollide(x-4,y)
		b = ccollide(x+4,y)
		return a,b
	end
end
function ucol()
	a,b = ycol(p.x,p.y)
	return ft(a) or ft(b)
end
function dcol()
	a,b = ycol(p.x,p.y)
	return ft(a) or ft(b)
end

function xcol(x,y)
	if x % 8 != 0 then
		return bg
	else
		return ccollide(x,y)
	end
end
function lcol()
	return ft(xcol(p.x-8,p.y))
end
function rcol()
	return ft(xcol(p.x+8,p.y))
end

function checkforcoin()
	if xcol(p.x,p.y) == 32 then
		psnd(snd.coin)
		mset(p.x/8,p.y/8,bg)
		wait.f = 10
		wait.call = coinup
	end
end

function updatemovement()
	updatejump()
	updatewalk()
	checkforcoin()
end

