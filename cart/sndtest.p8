function loadsnds()
	for k,v in pairs(brosnd) do
		bytes = {ord(v,1,#v)}
		samples = {}
		for b in all(bytes) do
			for shift=6,0,-2 do
				bits = (b>>shift) & 3
				add(samples,amp[bits])
			end
		end
		brosnd[k] = samples
	end
end

function playsnd(name)
	snd = brosnd[name]
	sndlen = #snd
	p = 256
	for i=1,#snd,256 do
		sndend = i + 256
		if sndlen < sndend then
			p = sndend - i
			sndend = sndlen
		end

		adr = 0x4300
		for j=i,sndend do
			poke(adr,snd[i])
			adr += 1
		end

		--play
		serial(0x808,0x4300,p)
		while stat(108)!=0 do end
	end
end

loadsnds()
playsnd("dies")

